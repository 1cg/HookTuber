{
  "name": "Hooktuber",
  "description": "Swaps video players and source on youtube.com to hooktube ",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/watch*"
      ],
      "run_at": "document_start",
      "css": ["player.css"],
      "js": [
        "main.js"
      ]
    }
  ],
  "manifest_version": 2
}
